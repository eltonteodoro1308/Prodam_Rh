			<input name='bimprimir' id='bimprimir' type='button' value='imprimir' class='button Botoes' onclick='Imprime()'/>
		</div>
		
		<%
			HttpSession->xMatricula := Nil
			HttpSession->xNomeFun  	:= Nil
		%>
		
		<br/>
		
						
	</div>
